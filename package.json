{
  "name": "maiyue-ai-extension",
  "version": "1.0.0",
  "description": "迈越浏览器AI助手",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "watch": "vite build --watch --emptyOutDir",
    "dev:watch": "concurrently \"npm run dev\" \"npm run watch\"",
    "amis": "npm install amis@latest amis-app-layout@latest amis-core@latest amis-editor@latest amis-editor-core@latest amis-formula@latest amis-theme-editor@latest amis-theme-editor-helper@latest amis-ui@latest --force"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@microsoft/fetch-event-source": "^2.0.1",
    "amis": "^29.1.10",
    "amis-app-layout": "^29.0.3",
    "amis-core": "^29.0.5",
    "amis-editor": "^29.0.6",
    "amis-editor-core": "^29.0.3",
    "amis-formula": "^29.0.3",
    "amis-theme-editor": "^29.0.4",
    "amis-theme-editor-helper": "^29.0.3",
    "amis-ui": "^29.1.1",
    "mobx": "^4.15.7",
    "mobx-react": "^6.3.1",
    "mobx-state-tree": "^3.17.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
